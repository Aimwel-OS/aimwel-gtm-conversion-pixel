___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Required necessary APIs
const log = require('logToConsole');
const getType = require('getType');
const sendPixel = require('sendPixel');
const encodeUri = require('encodeUri');
const encodeUriComponent = require('encodeUriComponent');
const getTimestampMillis = require('getTimestampMillis');
var currentTimestampInMilliseconds = getTimestampMillis();

// Adding query string in URL and adding non-optional timestamp
var url = encodeUri(data.aimwel_api_endpoint) + '?' + "timestamp=" + currentTimestampInMilliseconds + "&";

// Looping through field input variables and adding them to the URL
data.utmParameters.forEach(function(element){
  if ((getType(element.upn) !== 'undefined') || (getType(element.upv) !== 'undefined')){
    url += encodeUriComponent(element.upn) +"="+ encodeUriComponent(element.upv) +"&";
  } else {
    log('Field in UTM Parameter section is undefined');
  }
});

data.aimwelParameters.forEach(function(element){
  if ((getType(element.apn) !== 'undefined') || (getType(element.apv) !== 'undefined')){
    url += encodeUriComponent(element.apn) +"="+ encodeUriComponent(element.apv) +"&";
  } else {
    log('Field in Aimwel Parameter section is undefined');
  }
});

data.platformParameters.forEach(function(element){
  if ((getType(element.ppn) !== 'undefined') || (getType(element.ppv) !== 'undefined')){
    url += encodeUriComponent(element.ppn) +"="+ encodeUriComponent(element.ppv) +"&";
  } else {
    log('Field in Platform Parameter section is undefined');
  }
});

data.channelParameters.forEach(function(element){
  if ((getType(element.cpn) !== 'undefined') || (getType(element.cpv) !== 'undefined')){
    url += encodeUriComponent(element.cpn) +"="+ encodeUriComponent(element.cpv) +"&";
  } else {
    log('Field in Aimwel Parameter section is undefined');
  }
});

// Remove trailing ampersand
url = url.substring(0, url.length - 1);

log(url);

// Send GET request
sendPixel(url, data.gtmOnSuccess, data.gtmOnFailure);


